package goldmine;

import java.util.Arrays;
import java.util.List;
import java.util.ArrayList;

public class mine {

	public int[] getAllocation(String[] mines, int miners){
		
		
		return new int[]{2,2};
	}
	
	public List<List<Integer>> convert(String[] mines){
		int n=mines.length;
		List<List<Integer>> ret= new ArrayList<List<Integer>>(n);
		
		for (int i=0;i<n;i++){
			Arrays.stream(mines[i].split(", ")).mapToInt(Integer::parseInt).toArray();
		}
		
		return ret;
	}
	
	
	public static void main(String [ ] args) {
		String[] test1={ "000, 030, 030, 040, 000, 000, 000",
			 			 "020, 020, 020, 010, 010, 010, 010" };
		mine solution=new mine();
		int[] actual = solution.getAllocation(test1,4);
//		System.out.println(test1.length);
//		System.out.println(Arrays.toString(actual));
	}
}



